{
  "name": "store-manager",
  "version": "1.0.0",
  "description": "Store Manager is a web application that allows store owners to manage product sales, inventory and \r records",
  "main": "app.js",
  "scripts": {
    "test-dev": "export current_env=test || SET \"current_env=test\" && npm run build && nyc --reporter=lcov --reporter=text --cache mocha dist/test/*.test.js --exit --timeout=20000",
    "test": "export current_env=test || SET \"current_env=test\" && npm run test-tables-setup && npm run build && nyc --reporter=lcov --reporter=text --cache mocha dist/test/*.test.js --exit --timeout=20000",
    "start": "npm run build && node dist/app.js",
    "lint": "./node_modules/.bin/eslint server --fix",
    "clean": "rm -rf dist",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "npm run clean && mkdir dist && babel server -s -d dist",
    "tables-setup": "psql -U postgres -f server/models/tables-setup.sql",
    "test-tables-setup": "psql -U postgres -f server/models/tables-setup.sql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rajeman/Store-Manager.git"
  },
  "author": "rajeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rajeman/Store-Manager/issues"
  },
  "homepage": "https://github.com/rajeman/Store-Manager#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "pg": "^7.5.0",
    "validator": "^10.8.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.7.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1",
    "supertest": "^3.3.0"
  },
  "engines": {
    "node": "10.6.0"
  }
}
